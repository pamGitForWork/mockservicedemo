<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessageSetResponseMessage">
    <DisplayName>AssignMessage.SetResponseMessage</DisplayName>
    <Properties/>
    <Set>
        <StatusCode>{var.statusCode}</StatusCode>
        <Payload contentType="application/json">{var.response}</Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="true" transport="http" type="response"/>
</AssignMessage>